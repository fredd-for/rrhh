--

CREATE TABLE evento (
    id integer NOT NULL,
    evento character varying(200) NOT NULL,
    regional_id integer NOT NULL,
    direccion character varying(200) NOT NULL,
    tipo_evento_id integer NOT NULL,
    tipo_asistencia_id integer NOT NULL,
    tipo_capacitacion_id integer NOT NULL,
    fecha_ini timestamp(6) without time zone NOT NULL,
    fecha_fin timestamp(6) without time zone NOT NULL,
    hora_ini timestamp(6) without time zone,
    hora_fin timestamp(6) without time zone,
    carga_horaria integer,
    costo_unitario integer,
    costo_total integer,
    cupo integer,
    tipo_pago integer NOT NULL,
    persona_id integer NOT NULL,
    observacion character varying(200),
    estado integer NOT NULL,
    visible integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
    fecha_reg timestamp(6) without time zone NOT NULL,
    user_mod_id integer,
    fecha_mod timestamp(6) without time zone
);


