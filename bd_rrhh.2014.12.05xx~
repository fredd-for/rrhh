    e_civil character varying(30),
    codigo character varying(30),
    nacionalidad character varying(30),
    nit integer,
    num_func_sigma integer,
    grupo_sanguineo character varying(30),
    num_lib_ser_militar character varying(50),
    num_reg_profesional character varying(50),
    observacion character varying(200),
