--

CREATE TABLE seguimientos (
    id integer NOT NULL,
    pac_id integer NOT NULL,
    proceso_contratacion_id integer NOT NULL,
    codigo_proceso character varying(50) NOT NULL,
    codigo_cargo character varying(50),
    cargo character varying(255),
    sueldo numeric(10,2),
    nro_solicitud character varying(100),
    fecha_sol date,
    cert_presupuestaria character varying(100),
    fecha_cert_pre date,
    vacante integer,
    haber_basico numeric(10,2),
    fecha_apr_mae date,
    tipo_contratacion character varying(50),
    fecha_apr timestamp without time zone,
    seguimiento_estado_id integer NOT NULL,
    user_reg_id integer NOT NULL,
    baja_logica integer DEFAULT 1 NOT NULL,
    fecha_reg timestamp without time zone NOT NULL,
    organigrama_id integer,
    usuario_sol character varying(150)
);


