--
-- TOC entry 412 (class 1255 OID 33018)
-- Name: f_listado_gestiones(integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_listado_gestiones(i_id_persona integer, OUT id_contrato integer, OUT gestion integer) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_listado_gestiones
 Descripción: Función para obtener el listado de gestiones de acuerdo a las relaciones laborales registradas en el sistema. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      05/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
SELECT r.id as id_contrato, 
  EXTRACT(YEAR from fecha_ini)::int as gestion
  FROM relaborales r
  WHERE r.estado>=0 AND r.baja_logica=1
  AND r.persona_id=i_id_persona
  ORDER BY gestion DESC;
  $$;


ALTER FUNCTION public.f_listado_gestiones(i_id_persona integer, OUT id_contrato integer, OUT gestion integer) OWNER TO postgres;

--
-- TOC entry 409 (class 1255 OID 22027)
-- Name: f_obtener_edad(date, date); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_obtener_edad(pd_fecha_ini date, pd_fecha_fin date, OUT pn_edad integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
BEGIN
/********************************************************************************************************************************************
 Nombre: f_obtener_edad
 Función para obtener los años transcurridos entre dos fechas, su uso comun tiene que ver con la obtención de la edad de las personas.
 Historial de Creación y Modificaciones:
   Ver        Fecha         Autor           Descripción
 ---------  ----------  ---------------  ------------------------------------
   1.0      17/10/2014  Javier Loza      Creación de la función.
 *********************************************************************************************************************************************/
pn_edad := FLOOR(((DATE_PART('YEAR',pd_fecha_fin)-DATE_PART('YEAR',pd_fecha_ini))* 372 + (DATE_PART('MONTH',pd_fecha_fin) - DATE_PART('MONTH',pd_fecha_ini))*31 + (DATE_PART('DAY',pd_fecha_fin)-DATE_PART('DAY',pd_fecha_ini)))/372);
END;
$$;


ALTER FUNCTION public.f_obtener_edad(pd_fecha_ini date, pd_fecha_fin date, OUT pn_edad integer) OWNER TO postgres;

--
-- TOC entry 425 (class 1255 OID 23608)
-- Name: f_organigramas(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_organigramas(OUT padre_id integer, OUT id integer, OUT unidad_administrativa character varying, OUT enlace text) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_organigramas
 Descripción: Función para obtener el listado de unidades administrativas con los enlaces correspondientes. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
    1.0      20/10/2014  Javier Loza     Creación de la función.
***********************************************************************************/
WITH RECURSIVE arbol (padre_id, id) AS (  
    SELECT  
    n.padre_id,  
    n.id,
    n.unidad_administrativa,
    case when ne.nivel_estructural like  'GERENCIA EJECUTIVA' then CONCAT(CONCAT('<u>',n.unidad_administrativa),'</u>')
    when ne.nivel_estructural like  'GERENCIA' then CONCAT(CONCAT('<u>',n.unidad_administrativa),'</u>')
    when ne.nivel_estructural like  'DEPARTAMENTO' then CONCAT(CONCAT('<li>',n.unidad_administrativa),'</li>')
    else '' end as enlace
    FROM  
    organigramas n  
    inner join nivelestructurales ne on n.nivel_estructural_id=ne.id
  UNION ALL  
    SELECT n2.padre_id, n2.id,n2.unidad_administrativa,
    case when nes.nivel_estructural like  'GERENCIA EJECUTIVA' then CONCAT(CONCAT('<u>',n2.unidad_administrativa),'</u>')
    when nes.nivel_estructural like  'GERENCIA' then CONCAT(CONCAT('<u>',n2.unidad_administrativa),'</u>')
    when nes.nivel_estructural like  'DEPARTAMENTO' then CONCAT(CONCAT('<li>',n2.unidad_administrativa),'</li>')
    else '' end as enlace
    FROM arbol a 
    INNER JOIN organigramas n2  
      ON (n2.id = a.padre_id)  
    INNER JOIN nivelestructurales nes on n2.nivel_estructural_id=nes.id
  )  
SELECT distinct * FROM arbol
where padre_id is not null
order by padre_id,id;


$$;


ALTER FUNCTION public.f_organigramas(OUT padre_id integer, OUT id integer, OUT unidad_administrativa character varying, OUT enlace text) OWNER TO postgres;

--
-- TOC entry 426 (class 1255 OID 32053)
-- Name: f_personas(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_personas(OUT id_persona integer, OUT postulante_id integer, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT codigo character varying, OUT nacionalidad character varying, OUT nit character varying, OUT num_func_sigma integer, OUT grupo_sanguineo character varying, OUT num_lib_ser_militar character varying, OUT num_reg_profesional character varying, OUT observacion character varying, OUT estado integer, OUT baja_logica integer, OUT agrupador integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT direccion_dom character varying, OUT telefono_fijo character varying, OUT telefono_inst character varying, OUT telefono_fax character varying, OUT interno_ins character varying, OUT celular_per character varying, OUT celular_inst character varying, OUT num_credencial character varying, OUT ac_no character varying, OUT e_mail_per character varying, OUT e_mail_inst character varying, OUT contacto_observacion character varying, OUT contacto_estado integer) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_personas
 Descripción: Función para obtener el listado de personas adicionando los registros de contacto si los tuviera. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      29/10/2014  Javier Loza     Creación de la función.
***********************************************************************************/
SELECT p.id, p.postulante_id, p.p_nombre, p.s_nombre, p.t_nombre, p.p_apellido, p.
       s_apellido, p.c_apellido, p.tipo_doc,p.ci, p.expd, p.num_complemento, p.fecha_nac, f_obtener_edad(p.fecha_nac,current_date) as edad,p.
       lugar_nac, p.genero, p.e_civil, p.codigo, p.nacionalidad, p.nit, p.num_func_sigma, p.
       grupo_sanguineo, p.num_lib_ser_militar, p.num_reg_profesional, p.observacion, p.
       estado, p.baja_logica, p.agrupador, p.user_reg_id, p.fecha_reg, p.user_mod_id, p.
       fecha_mod,
     pc.direccion_dom, pc.telefono_fijo, pc.telefono_inst, pc.telefono_fax, pc.interno_inst, 
       pc.celular_per, pc.celular_inst, pc.num_credencial, pc.ac_no, pc.e_mail_per, pc.e_mail_inst, 
       pc.observacion as contacto_observacion, pc.estado as contacto_estado
  FROM personas p
  left join personascontactos pc 
  on p.id = pc.persona_id and pc.estado=1 and p.baja_logica=1;
  $$;


ALTER FUNCTION public.f_personas(OUT id_persona integer, OUT postulante_id integer, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT codigo character varying, OUT nacionalidad character varying, OUT nit character varying, OUT num_func_sigma integer, OUT grupo_sanguineo character varying, OUT num_lib_ser_militar character varying, OUT num_reg_profesional character varying, OUT observacion character varying, OUT estado integer, OUT baja_logica integer, OUT agrupador integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT direccion_dom character varying, OUT telefono_fijo character varying, OUT telefono_inst character varying, OUT telefono_fax character varying, OUT interno_ins character varying, OUT celular_per character varying, OUT celular_inst character varying, OUT num_credencial character varying, OUT ac_no character varying, OUT e_mail_per character varying, OUT e_mail_inst character varying, OUT contacto_observacion character varying, OUT contacto_estado integer) OWNER TO postgres;

--
-- TOC entry 416 (class 1255 OID 57808)
-- Name: f_relaborales(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_relaborales(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT soelabocontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT organigrama_regional character varying, OUT id_regional integer, OUT regional character varying, OUT regional_codigo integer, OUT id_departamento integer, OUT departamento character varying, OUT id_provincia integer, OUT provincia character varying, OUT id_localidad integer, OUT localidad character varying, OUT residencia character varying, OUT unidad_ejecutora character varying, OUT cod_ue integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT unidades_superiores character varying, OUT unidades_dependientes character varying, OUT partida integer, OUT fuente_codigo integer, OUT fuente character varying, OUT organismo_codigo integer, OUT organismo character varying, OUT observacion character varying, OUT estado integer, OUT estado_descripcion text, OUT estado_abreviacion text, OUT tiene_contrato_vigente integer, OUT id_eventual integer, OUT id_consultor integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT persona_user_reg_id integer, OUT persona_fecha_reg timestamp without time zone, OUT persona_user_mod_id integer, OUT persona_fecha_mod timestamp without time zone) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_relaborales
 Descripción: Función para obtener los registros laborales en el sistema Oasis. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      20/10/2014  Javier Loza     Creación de la función.
     1.1      13/11/2014  Javier Loza	  Se cambia el modo de ver el campo nombres, haciendo que se vea p_apellido, s_apellido, c_apellido, p_nombre, s_nombre, t_nombre	
***********************************************************************************/
select relaborales.id as id_relaboral,
personas.id as id_persona,
personas.p_apellido||' '||
CASE WHEN personas.s_apellido IS NOT NULL THEN personas.s_apellido ELSE '' END 
||
CASE WHEN personas.c_apellido IS NOT NULL THEN ' '||personas.c_apellido ELSE '' END 
||
CASE WHEN personas.p_nombre IS NOT NULL THEN ' '||personas.p_nombre ELSE '' END 
||
CASE WHEN personas.s_nombre IS NOT NULL THEN ' '||personas.s_nombre ELSE '' END 
||
CASE WHEN personas.t_nombre IS NOT NULL THEN ' '||personas.t_nombre ELSE '' END 
as nombres,

personas.p_nombre,
personas.s_nombre,
personas.t_nombre,
personas.p_apellido,
personas.s_apellido,
personas.c_apellido,
personas.tipo_doc as tipo_documento,
personas.ci,
personas.expd,
personas.num_complemento,
personas.fecha_nac,
f_obtener_edad(personas.fecha_nac,current_date) as edad,
personas.lugar_nac,
personas.genero,
personas.e_civil,
case when condiciones.id = 1 then cast(cargos.codigo as integer) else null end as item,
relaborales.carrera_adm,
relaborales.num_contrato,
case when relaboralesnumeradores.relaboral_id >0 then 
relaboralesnumeradores.estado
when relaboralesnumeradores.relaboral_id is null then -1
else 0 end as contratonumerador_estado,
solelabcontratos.id as id_solelabcontrato,
tempregionales.sigla as solelabcontrato_regional_sigla,
solelabcontratos.numero as solelabcontrato_numero,
solelabcontratos.gestion as solelabcontrato_gestion,
case when solelabcontratos.id is not null
then concat(tempregionales.sigla,concat('-',concat(solelabcontratos.numero,concat('/',solelabcontratos.gestion))))
else null end as solelabcontrato_codigo,
case when solelabcontratos.id is not null
then solelabcontratos.user_reg_id
else null end as  solelabcontrato_user_reg_id,
case when solelabcontratos.id is not null
then solelabcontratos.fecha_reg
else null end as solelabcontrato_fecha_sol,
relaborales.fecha_ini,
relaborales.fecha_incor,
relaborales.fecha_fin,
relaborales.fecha_baja,
relaborales.fecha_ren,
relaborales.fecha_acepta_ren,
relaborales.fecha_agra_serv,
motivosbajas.motivo_baja,
motivosbajas.abreviacion as motivosbajas_abreviacion, 
relaborales.descripcion_baja,
relaborales.descripcion_anu,
cargos.id as id_cargo,
cargos.codigo as cargo_codigo,
cargos.cargo,
nivelsalariales.id as id_nivelessalarial,
nivelsalariales.denominacion as nivelsalarial,
nivelsalariales.resolucion_id as nivelsalarial_resolucion_id,
nivelsalariales.agrupador as numero_escala,
nivelsalariales.gestion as gestion_escala,
nivelsalariales.sueldo,
procesoscontrataciones.id as id_procesocontratacion,
procesoscontrataciones.codigo_proceso as proceso_codigo,
procesoscontrataciones.codigo_convocatoria as convocatoria_codigo,
tiposconvocatorias.tipo as convocatoria_tipo,
finpartidas.id as id_fin_partida,
finpartidas.denominacion as fin_partida,
condiciones.id as id_condicion,
condiciones.condicion,
CAST('ADMINISTRATIVO' as character varying(30)) as categoria_relaboral,
das.id as id_da,
das.direccion_administrativa,
tempregionalesorganigrama.id as organigrama_regional_id,
tempregionalesorganigrama.regional as organigrama_regional,
regionales.id as id_regional,
regionales.regional,
regionales.agrupador as regional_codigo,
departamentos.id as id_departamento,
departamentos.departamento,
provincias.id as id_provincia,
case when provincias.capital is not null then
concat(provincias.provincia,concat(' - ',provincias.capital)) 
else provincias.provincia end as provincia,
localidades.id as id_localidad,
localidades.localidad,
case when localidades.id is not null then localidades.localidad
when provincias.id is not null then provincias.capital
else departamentos.departamento end as residencia,
ejecutoras.unidad_ejecutora,
ejecutoras.cod_ue,
case when 
temporganigramas.unidad_administrativa is not null
then 
case when organigramas.unidad_administrativa like 'GERENCIA%'
then organigramas.id
else temporganigramas.id end 
else organigramas.id 
end as id_gerencia_administrativa,
case when 
temporganigramas.unidad_administrativa is not null
then 
case when organigramas.unidad_administrativa like 'GERENCIA%'
then organigramas.unidad_administrativa
else temporganigramas.unidad_administrativa end 
else organigramas.unidad_administrativa 
end as gerencia_administrativa,
case when temporganigramas.unidad_administrativa is not null
then 
case when organigramas.unidad_administrativa like 'GERENCIA%'
then null
else organigramas.id end
else null 
end as id_departamento_administrativo,
case when temporganigramas.unidad_administrativa is not null
then 
case when organigramas.unidad_administrativa like 'GERENCIA%'
then ''
else organigramas.unidad_administrativa end
else '' 
end as departamento_administrativo,
organigramas.id as id_organigrama,
organigramas.unidad_administrativa,
organigramas.sigla as organigrama_sigla,
organigramas.orden as organigrama_orden,
tempareas.id as id_area,
tempareas.unidad_administrativa as area,
u.id as id_ubicacion,
u.ubicacion,
cast('' as character varying(300)) as unidades_superiores,
cast('' as character varying(300)) as unidades_dependientes,
/*(select f_obt_cadena_unidades_sup(organigramas.id) from dual) as unidades_superiores,
(select concat(concat(',',f_obt_cadena_unidades_sup(organigramas.id)),',') from dual) as unidades_superiores_comas,
(select f_obt_cadena_unidades_dep(organigramas.id) from dual) as unidades_dependientes,
(select concat(concat(',',f_obt_cadena_unidades_dep(organigramas.id)),',') from dual) as unidades_dependientes_comas,
*/
finpartidas.partida,
fuentes.codigo as fuente_codigo,
fuentes.fuente,
organismos.codigo as organismo_codigo,
organismos.organismo,
relaborales.observacion,
relaborales.estado,
case when relaborales.estado=0 then 'PASIVO' when relaborales.estado=1 then 'ACTIVO' when relaborales.estado=2 then 'EN PROCESO' end as estado_descripcion,
case when relaborales.estado=0 then '[P]' when relaborales.estado=1 then '[A]' when relaborales.estado=2 then '[EP]' end as estado_abreviacion,
f_tiene_relaboral_vigente(personas.id) as tiene_contrato_vigente,
case when relaboraleseventuales.id is not null then relaboraleseventuales.id else 0 end as id_eventual,
case when relaboralesconsultores.id is not null then relaboralesconsultores.id else 0 end as id_consultor,
relaborales.user_reg_id,
relaborales.fecha_reg,
relaborales.user_mod_id,
relaborales.fecha_mod,
personas.user_reg_id as persona_user_reg_id,
personas.fecha_reg as persona_fecha_reg ,
personas.user_mod_id  as persona_user_mod_id,
personas.fecha_mod as persona_fecha_mod
from personas 
inner join relaborales on personas.id=relaborales.persona_id
inner join cargos on relaborales.cargo_id=cargos.id
left join motivosbajas on relaborales.motivobaja_id = motivosbajas.id
--inner join categorias on categorias.id=cargos.categoria_id
--inner join categorias on categorias.
inner join das on das.id=relaborales.da_id
inner join regionales on relaborales.regional_id=regionales.id
inner join departamentos on departamentos.id=regionales.departamento_id
left join provincias on regionales.provincia_id=provincias.id
left join localidades on regionales.localidad_id=localidades.id
inner join organigramas on relaborales.organigrama_id=organigramas.id
left join organigramas temporganigramas on temporganigramas.id = organigramas.padre_id
inner join regionales tempregionalesorganigrama on organigramas.regional_id=tempregionalesorganigrama.id
left join procesoscontrataciones on procesoscontrataciones.id=relaborales.procesocontratacion_id
--left join convocatorias on convocatorias.id=procesos.convocatoria_id
left join tiposconvocatorias on tiposconvocatorias.id = procesoscontrataciones.tipoconvocatoria_id
left join ejecutoras on ejecutoras.id=relaborales.ejecutora_id
left join finpartidas on finpartidas.id=relaborales.finpartida_id
left join financiamientos on financiamientos.id=finpartidas.financiamiento_id
left join proyectos on financiamientos.proyecto_id=financiamientos.id
left join organismos on financiamientos.organismo_id=organismos.id
left join fuentes on financiamientos.fuente_id=fuentes.id
left join condiciones on condiciones.id = finpartidas.condicion_id
left join nivelsalariales on nivelsalariales.id=relaborales.nivelsalarial_id
left join personascontactos on personascontactos.persona_id = personas.id and personascontactos.estado=1
left join solelabcontratos on solelabcontratos.id=relaborales.solelabcontrato_id
left join regionales tempregionales on tempregionales.id = solelabcontratos.regional_id
left join relaboralesnumeradores on relaboralesnumeradores.relaboral_id = relaborales.id
left join relaboraleseventuales on relaboraleseventuales.relaboral_id = relaborales.id
left join relaboralesconsultores on relaboralesconsultores.relaboral_id = relaborales.id
left join relaboralesareas on relaborales.id=relaboralesareas.relaboral_id and relaboralesareas.estado=1 and relaboralesareas.baja_logica=1
left join organigramas tempareas on tempareas.id = relaboralesareas.organigrama_id
left join nivelestructurales ne on ne.id = tempareas.nivel_estructural_id and ne.nivel_estructural LIKE 'AREA'
left join relaboralesubicaciones ru on ru.relaboral_id = relaborales.id
left join ubicaciones u on u.id = ru.ubicacion_id
where relaborales.baja_logica=1
order by personas.p_apellido,personas.s_apellido,personas.c_apellido,personas.p_nombre,personas.s_nombre,personas.t_nombre,
relaborales.da_id,relaborales.fecha_ini;
$$;


ALTER FUNCTION public.f_relaborales(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT soelabocontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT organigrama_regional character varying, OUT id_regional integer, OUT regional character varying, OUT regional_codigo integer, OUT id_departamento integer, OUT departamento character varying, OUT id_provincia integer, OUT provincia character varying, OUT id_localidad integer, OUT localidad character varying, OUT residencia character varying, OUT unidad_ejecutora character varying, OUT cod_ue integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT unidades_superiores character varying, OUT unidades_dependientes character varying, OUT partida integer, OUT fuente_codigo integer, OUT fuente character varying, OUT organismo_codigo integer, OUT organismo character varying, OUT observacion character varying, OUT estado integer, OUT estado_descripcion text, OUT estado_abreviacion text, OUT tiene_contrato_vigente integer, OUT id_eventual integer, OUT id_consultor integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT persona_user_reg_id integer, OUT persona_fecha_reg timestamp without time zone, OUT persona_user_mod_id integer, OUT persona_fecha_mod timestamp without time zone) OWNER TO postgres;

--
-- TOC entry 421 (class 1255 OID 58964)
-- Name: f_relaborales_movilidad(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_relaborales_movilidad(OUT id_relaboral integer, OUT id_relaboralmovilidad integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT numero integer, OUT cargo character varying, OUT evento_id integer, OUT evento character varying, OUT motivo character varying, OUT id_pais integer, OUT pais character varying, OUT id_departamento integer, OUT departamento character varying, OUT lugar character varying, OUT fecha_ini date, OUT hora_ini time without time zone, OUT fecha_fin date, OUT hora_fin time without time zone, OUT id_memorandum integer, OUT id_tipomemorandum integer, OUT tipo_memorandum character varying, OUT memorandum_correlativo character varying, OUT memorandum_gestion integer, OUT fecha_mem timestamp without time zone, OUT observacion character varying, OUT estado integer, OUT estado_descripcion character varying) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_relaborales_movilidad
 Descripción: Función para obtener los registros de movilidad funcionaria de acuerdo a los registro de relación laboral válidos. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      17/11/2014  Javier Loza     Creación de la función.    
***********************************************************************************/
select r.id as id_relaboral,
rm.id as id_relaboralmovilidad,
case when 
tor.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then o.id
else tor.id end 
else o.id 
end as id_gerencia_administrativa,
case when 
tor.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then o.unidad_administrativa
else tor.unidad_administrativa end 
else o.unidad_administrativa 
end as gerencia_administrativa,
case when tor.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then null
else o.id end
else null 
end as id_departamento_administrativo,
case when tor.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then ''
else o.unidad_administrativa end
else '' 
end as departamento_administrativo,
o.id as id_organigrama,
o.unidad_administrativa,
o.sigla as organigrama_sigla,
o.orden as organigrama_orden,
tempareas.id as id_area,
tempareas.unidad_administrativa as area,
u.id as id_ubicacion,
u.ubicacion,
rm.numero,
rm.cargo,
rm.evento_id,
cast('' as character varying) as evento,
rm.motivo,
rm.pais_id,
ps.pais,
rm.departamento_id,
ds.departamento,
rm.lugar,
rm.fecha_ini,
rm.hora_ini,
rm.fecha_fin,
rm.hora_fin,
m.id as id_memorandum,
tm.id as id_tipomemorandum,
tm.tipo_memorandum,
m.correlativo as memorandum_correlativo,
m.gestion as memorandum_gestion,
m.fecha_mem,
rm.observacion,
rm.estado,
case when rm.estado=1 then 'ACTIVO'
when rm.estado=0 then 'PASIVO'
else '' end as estado_descripcion
from relaboralesmovilidades rm
inner join relaborales r on r.id = rm.relaboral_id
inner join das d on d.id=rm.da_id
inner join regionales rg on rm.regional_id=rg.id
inner join departamentos dp on dp.id=rg.departamento_id
left join provincias p on rg.provincia_id=p.id
left join localidades l on rg.localidad_id=l.id
left join organigramas o on rm.organigrama_id=o.id
left join organigramas tor on tor.id = o.padre_id
left join ubicaciones u on u.id = rm.ubicacion_id
left join organigramas tempareas on tempareas.id = rm.area_id
inner join modalidadmemorandum mm on mm.id = rm.modalidadmemorandum_id 
inner join tiposmemorandums tm on tm.id = mm.tipomemorandum_id
inner join memorandums m on m.id = rm.memorandum_id and m.tipomemorandum_id = tm.id
left join paises ps on ps.id = rm.pais_id
left join departamentos ds on ds.id = rm.departamento_id
order by rm.numero,m.fecha_mem;
$$;


ALTER FUNCTION public.f_relaborales_movilidad(OUT id_relaboral integer, OUT id_relaboralmovilidad integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT numero integer, OUT cargo character varying, OUT evento_id integer, OUT evento character varying, OUT motivo character varying, OUT id_pais integer, OUT pais character varying, OUT id_departamento integer, OUT departamento character varying, OUT lugar character varying, OUT fecha_ini date, OUT hora_ini time without time zone, OUT fecha_fin date, OUT hora_fin time without time zone, OUT id_memorandum integer, OUT id_tipomemorandum integer, OUT tipo_memorandum character varying, OUT memorandum_correlativo character varying, OUT memorandum_gestion integer, OUT fecha_mem timestamp without time zone, OUT observacion character varying, OUT estado integer, OUT estado_descripcion character varying) OWNER TO postgres;

