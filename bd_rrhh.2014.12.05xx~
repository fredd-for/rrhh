CREATE FUNCTION f_areas(id_padre integer, OUT id_area integer, OUT padre_id integer, OUT gestion integer, OUT da_id integer, OUT regional_id integer, OUT unidad_administrativa character varying, OUT nivel_estructural_id integer, OUT sigla character varying, OUT fecha_ini date, OUT fecha_fin date, OUT orden integer, OUT observacion character varying, OUT estado integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT area_sustantiva integer) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_areas
 Descripción: Función para obtener el listado de áreas administrativas de acuerdo al identificador de organigrama enviado en el primer parámetro de entrada.
	      En caso de que el primer parámetro de ingreso sea igual a cero, se muestra todas las áreas disponibles o activas en el organigrama.
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      07/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
select o.id, 
o.padre_id, 
o.gestion, 
o.da_id, 
o.regional_id, 
o.unidad_administrativa, 
o.nivel_estructural_id, 
o.sigla, 
o.fecha_ini, 
o.fecha_fin, 
o.orden, 
o.observacion, 
o.estado, 
o.baja_logica, 
o.user_reg_id, 
o.fecha_reg, 
o.user_mod_id, 
o.fecha_mod, 
o.area_sustantiva 
from organigramas o 
inner join nivelestructurales n on o.nivel_estructural_id = n.id
where o.padre_id= case when id_padre >0 then id_padre else o.padre_id end
and n.nivel_estructural LIKE 'AREA'
and n.estado=1 and n.baja_logica=1;
$$;


ALTER FUNCTION public.f_areas(id_padre integer, OUT id_area integer, OUT padre_id integer, OUT gestion integer, OUT da_id integer, OUT regional_id integer, OUT unidad_administrativa character varying, OUT nivel_estructural_id integer, OUT sigla character varying, OUT fecha_ini date, OUT fecha_fin date, OUT orden integer, OUT observacion character varying, OUT estado integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT area_sustantiva integer) OWNER TO postgres;

--
-- TOC entry 410 (class 1255 OID 59182)
-- Name: f_baja_movilidad_debido_baja_relaboral(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_baja_movilidad_debido_baja_relaboral() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
IF (TG_OP = 'UPDATE') THEN
	IF NEW.estado = 0 AND OLD.estado=1 THEN
		UPDATE relaboralesmovilidades SET estado=0,fecha_fin=NEW.fecha_baja, observacion = case when observacion is not null and observacion !='' then observacion||' - Baja de Movilidad debido a baja de relacion laboral.' else 'Baja de Movilidad debido a baja de relacion laboral.' end
		WHERE relaboralesmovilidades.relaboral_id = OLD.id AND relaboralesmovilidades.estado>=1;
	END IF;
RETURN NEW;
END IF;
END;
$$;


ALTER FUNCTION public.f_baja_movilidad_debido_baja_relaboral() OWNER TO postgres;

--
-- TOC entry 419 (class 1255 OID 57944)
-- Name: f_cargo_inmediato_superior(integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_cargo_inmediato_superior(id_cargo integer DEFAULT 0, OUT id integer, OUT organigrama_id integer, OUT ejecutora_id integer, OUT codigo character varying, OUT cargo character varying, OUT nivelsalarial_id integer, OUT cargo_estado_id integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT estado integer, OUT fin_partida_id integer, OUT depende_id integer, OUT formacion_requerida character varying) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_cargo_inmediato_superior
 Descripción: Función para obtener registro correspondiente al cargo del inmediato superior de acuerdo al identificador de un cargo.
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      21/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
select * from cargos cc where cc.id in (
select c.depende_id from cargos c 
where c.id = id_cargo);
$$;


ALTER FUNCTION public.f_cargo_inmediato_superior(id_cargo integer, OUT id integer, OUT organigrama_id integer, OUT ejecutora_id integer, OUT codigo character varying, OUT cargo character varying, OUT nivelsalarial_id integer, OUT cargo_estado_id integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT estado integer, OUT fin_partida_id integer, OUT depende_id integer, OUT formacion_requerida character varying) OWNER TO postgres;

--
-- TOC entry 423 (class 1255 OID 57945)
-- Name: f_cargo_inmediato_superior_relaboral(integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_cargo_inmediato_superior_relaboral(id_relaboral integer DEFAULT 0, OUT id integer, OUT organigrama_id integer, OUT ejecutora_id integer, OUT codigo character varying, OUT cargo character varying, OUT nivelsalarial_id integer, OUT cargo_estado_id integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT estado integer, OUT fin_partida_id integer, OUT depende_id integer, OUT formacion_requerida character varying) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_cargo_inmediato_superior_relaboral
 Descripción: Función para obtener registro correspondiente al cargo del inmediato superior considerando el identificador de la relación laboral.
	      Aunque su relación laboral no esté registrado en el sistema.
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      21/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
select * from cargos cc where cc.id in (
select c.depende_id from relaborales r 
inner join cargos c on c.id = r.cargo_id
where r.id = id_relaboral);
$$;


ALTER FUNCTION public.f_cargo_inmediato_superior_relaboral(id_relaboral integer, OUT id integer, OUT organigrama_id integer, OUT ejecutora_id integer, OUT codigo character varying, OUT cargo character varying, OUT nivelsalarial_id integer, OUT cargo_estado_id integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT estado integer, OUT fin_partida_id integer, OUT depende_id integer, OUT formacion_requerida character varying) OWNER TO postgres;

--
-- TOC entry 424 (class 1255 OID 23620)
-- Name: f_cargos(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_cargos(OUT id_cargo integer, OUT codigo character varying, OUT id_estado integer, OUT estado character varying, OUT id_finpartida integer, OUT finpartida character varying, OUT id_condicion integer, OUT condicion character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT cargo character varying, OUT sueldo numeric) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_cargos
 Descripción: Función para obtener el listado de cargos. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      20/10/2014  Javier Loza     Creación de la función.
     1.1      28/11/2014  Javier Loza     De acuerdo a modificaciones realizadas en la tabla cargos, se descarta la relación entre la columna nivelsalarial_id en la tabla cargos con la columna id de la tabla nivelsalariales.
					  Por otro lado, se adiciona la relación codigo_nivel(nueva) de la tabla cargo con la columna nivel de la tabla nivelsalariales.
***********************************************************************************/
select c.id as id_cargo,
c.codigo,
ce.id as id_estado,
ce.estado,
fp.id as id_finpartida,
fp.denominacion as finpartida,
cs.id as id_condicion,
cs.condicion,
o.id as id_organigrama,
o.unidad_administrativa,
case when 
temporganigramas.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then o.id
else temporganigramas.id end 
else o.id 
end as id_gerencia_administrativa,
case when 
temporganigramas.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then o.unidad_administrativa
else temporganigramas.unidad_administrativa end 
else o.unidad_administrativa 
end as gerencia_administrativa,
case when temporganigramas.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then null
else o.id end
else null 
end as id_departamento_administrativo,
case when temporganigramas.unidad_administrativa is not null
then 
case when o.unidad_administrativa like 'GERENCIA%'
then ''
else o.unidad_administrativa end
else '' 
end as departamento_administrativo,
n.id as id_nivelsalarial,
n.denominacion as nivelsalarial,
c.cargo,
n.sueldo
from cargos c
inner join organigramas o on c.organigrama_id = o.id
/*inner join nivelsalariales n on c.nivelsalarial_id = n.id*/
inner join nivelsalariales n on c.codigo_nivel = n.nivel
inner join ejecutoras e on c.ejecutora_id = e.id
inner join cargosestados ce on c.cargo_estado_id = ce.id
inner join finpartidas fp on c.fin_partida_id = fp.id
inner join condiciones cs on fp.condicion_id=cs.id
left join organigramas temporganigramas on temporganigramas.id = o.padre_id
where c.id not in (
	select distinct r.cargo_id from relaborales r	
	where r.baja_logica=1
	and r.estado>=1
);
$$;


ALTER FUNCTION public.f_cargos(OUT id_cargo integer, OUT codigo character varying, OUT id_estado integer, OUT estado character varying, OUT id_finpartida integer, OUT finpartida character varying, OUT id_condicion integer, OUT condicion character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT cargo character varying, OUT sueldo numeric) OWNER TO postgres;

