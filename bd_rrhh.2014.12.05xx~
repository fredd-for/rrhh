CREATE FUNCTION f_listado_gerencias(OUT id integer, OUT padre_id integer, OUT gestion integer, OUT da_id integer, OUT regional_id integer, OUT unidad_administrativa character varying, OUT nivel_estructural_id integer, OUT sigla character varying, OUT fecha_ini date, OUT fecha_fin date, OUT orden integer, OUT observacion character varying, OUT estado integer, OUT baja_logica integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT area_sustantiva integer, OUT visible integer) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombres: f_listado_gerencias
 Descripción: Función para la obtención del listado de gerencias administrativas vigentes en la empresa. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      19/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
select o.* from organigramas o
inner join nivelestructurales n on o.nivel_estructural_id = n.id
where n.nivel_estructural like 'GERENCIA%' and o.baja_logica=1 and o.estado=1
order by orden;
  $$;


