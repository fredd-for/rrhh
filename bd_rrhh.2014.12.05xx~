CREATE FUNCTION f_relaborales_y_personas_nuevas(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT soelabocontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT organigrama_regional character varying, OUT id_regional integer, OUT regional character varying, OUT regional_codigo integer, OUT id_departamento integer, OUT departamento character varying, OUT id_provincia integer, OUT provincia character varying, OUT id_localidad integer, OUT localidad character varying, OUT residencia character varying, OUT unidad_ejecutora character varying, OUT cod_ue integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT unidades_superiores character varying, OUT unidades_dependientes character varying, OUT partida integer, OUT fuente_codigo integer, OUT fuente character varying, OUT organismo_codigo integer, OUT organismo character varying, OUT observacion character varying, OUT estado integer, OUT estado_descripcion text, OUT estado_abreviacion text, OUT tiene_contrato_vigente integer, OUT id_eventual integer, OUT id_consultor integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT persona_user_reg_id integer, OUT persona_fecha_reg timestamp without time zone, OUT persona_user_mod_id integer, OUT persona_fecha_mod timestamp without time zone) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombres: f_relaborales_y_personas_nuevas
 Descripción: Función para obtener los registros laborales en el sistema Oasis con la adición de personas que no tengan ni un sólo registro de relación laboral. 
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      24/10/2014  Javier Loza     Creación de la función.
     1.1      13/11/2014  Javier Loza	  Se cambia el modo de ver el campo nombres, haciendo que se vea p_apellido, s_apellido, c_apellido, p_nombre, s_nombre, t_nombre	
***********************************************************************************/
select * from (select * from f_relaborales()
union 
select 
null as id_relaboral,
p.id as id_persona,
p.p_apellido||' '||
CASE WHEN p.s_apellido IS NOT NULL THEN p.s_apellido ELSE '' END 
||
CASE WHEN p.c_apellido IS NOT NULL THEN ' '||p.c_apellido ELSE '' END 
||
CASE WHEN p.p_nombre IS NOT NULL THEN ' '||p.p_nombre ELSE '' END 
||
CASE WHEN p.s_nombre IS NOT NULL THEN ' '||p.s_nombre ELSE '' END 
||
CASE WHEN p.t_nombre IS NOT NULL THEN ' '||p.t_nombre ELSE '' END 
as nombres,
p.p_nombre,
p.s_nombre,
p.t_nombre,
p.p_apellido,
p.s_apellido,
p.c_apellido,
p.tipo_doc as tipo_documento,
p.ci,
p.expd,
p.num_complemento,
p.fecha_nac,
f_obtener_edad(p.fecha_nac,current_date) as edad,
p.lugar_nac,
p.genero,
p.e_civil,
null as item,
null as carrera_adm,
null as num_contrato,
null as contrato_numerador_estado,
null as id_solelabcontrato,
null as solelabcontrato_regional_sigla,
null as solelabcontrato_numero,
null as solelabcontrato_gestion,
null as solelabcontrato_codigo,
null as solelabcontrato_user_reg_id,
null as solelabcontrato_fecha_sol,
null as fecha_ini,
null as fecha_incor,
null as fecha_fin,
null as fecha_baja,
null as fecha_ren,
null as fecha_acepta_ren,
null as fecha_agra_serv,
null as motivo_baja,
null as motivosbaja_abreviacion,
null as descripcion_baja,
null as descripcion_anu,
null as id_cargo,
null as cargo_codigo,
null as cargo,
null as id_nivelsalarial,
null as nivelsalarial,
null as nivelsalarial_resolucion_id,
null as numero_escala,
null as gestion_escala,
null as sueldo,
null as id_procesocontratacion,
null as proceso_codigo,
null as convocatoria_codigo,
null as convocatoria_tipo,
null as id_fin_partida,
null as fin_partida,
null as id_condicion,
null as condicion,
null as categoria_laboral,
null as id_da,
null as direccion_administrativa,
null as organigrama_regional_id,
null as organigrama_regional,
null as id_regional,
null as regional,
null as regional_codigo,
null as id_departamento,
null as departamento,
null as id_provincia,
null as provincia,
null as id_localidad,
null as localidad,
null as residencia,
null as unidad_ejecutora,
null as cod_ue,
null as id_gerencia_administrativa,
null as gerencia_administrativa,
null as id_departamento_administrativo,
null as departamento_administrativo,
null as id_organigrama,
null as unidad_administrativa,
null as organigrama_sigla,
null as organigrama_orden,
null as id_area,
null as area,
null as id_ubicacion,
null as ubicacion,
null as unidades_superiores,
null as unidades_dependientes,
null as partida,
null as fuente_codigo,
null as fuente,
null as organismo_codigo,
null as organismo,
null as observacion,
null as estado,
null as estado_descripcion,
null as estado_abreviacion,
-1 as tiene_contrato_vigente,
null as id_eventual,
null as id_consultor,
null as user_reg_id,
null as fecha_reg,
null as user_mod_id,
null as fecha_mod,
p.user_reg_id as persona_user_reg_id,
p.fecha_reg as persona_fecha_reg,
p.user_mod_id as persona_user_mod_id,
p.fecha_mod as persona_fecha_mod
from personas p
where p.id not in 
(
select distinct r.persona_id from relaborales r where r.baja_logica=1
)) as rp
order by p_apellido,s_apellido,c_apellido,p_nombre,s_nombre,t_nombre,
id_da,fecha_ini;
$$;


ALTER FUNCTION public.f_relaborales_y_personas_nuevas(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT soelabocontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT or