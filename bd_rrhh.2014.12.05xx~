--
-- TOC entry 312 (class 1259 OID 22509)
-- Name: personas; Type: TABLE; Schema: public; Owner: postgres; Tablespace: 
--

CREATE TABLE personas (
    id integer DEFAULT nextval('personas_id_seq'::regclass) NOT NULL,
    postulante_id integer,
    p_nombre character varying(50) NOT NULL,
    s_nombre character varying(50),
    t_nombre character varying(50),
    p_apellido character varying(50) NOT NULL,
    s_apellido character varying(50),
    c_apellido character varying(50),
    ci character varying(12) NOT NULL,
    expd character(2) NOT NULL,
    num_complemento character(2),
    fecha_nac date NOT NULL,
    lugar_nac character varying(100) NOT NULL,
    genero character(1) NOT NULL,
    e_civil character varying(30) NOT NULL,
    codigo character varying(30),
    nacionalidad character varying(30),
    nit character varying(15),
    num_func_sigma integer,
    grupo_sanguineo character varying(30),
    num_lib_ser_militar character varying(50),
    num_reg_profesional character varying(50),
    observacion character varying(200),
    estado integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
    fecha_reg timestamp(6) without time zone NOT NULL,
    user_mod_id integer,
    fecha_mod timestamp(6) without time zone,
    tipo_doc character varying(15),
    foto character varying(70) DEFAULT 'sin_foto.jpg'::character varying NOT NULL
);


ALTER TABLE public.personas OWNER TO postgres;

