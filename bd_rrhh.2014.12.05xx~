CREATE TABLE relaboralesmovilidades (
    id integer NOT NULL,
    relaboral_id integer NOT NULL,
    da_id integer NOT NULL,
    regional_id integer NOT NULL,
    cargo character varying(300),
    evento_id integer,
    motivo character varying(300),
    pais_id integer,
    departamento_id integer,
    lugar character varying(300),
    organigrama_id integer,
    area_id integer,
    ubicacion_id integer,
    modalidadmemorandum_id integer NOT NULL,
    memorandum_id integer,
    numero integer NOT NULL,
    fecha_ini date NOT NULL,
    hora_ini time without time zone,
    fecha_fin date,
    hora_fin time without time zone,
    observacion character varying(200),
    estado integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
    fecha_reg timestamp(6) without time zone NOT NULL,
    user_mod_id integer,
    fecha_mod timestamp(6) without time zone
);


