    tipofechaemidoc_id integer NOT NULL,
    tipoperssoldoc_id integer,
    ruta_carpeta character varying(200),
    nombre_carpeta character varying(200),
    formato_archivo_digital character varying(20),
    resolucion_archivo_digital integer,
    altura_archivo_digital integer,
    anchura_archivo_digital integer,
    campo_aux_a character varying(200),
    tipo_dato_campo_aux_a character varying(20),
    campo_aux_b character varying(200),
    tipo_dato_campo_aux_b character varying(20),
    campo_aux_c character varying(200),
    tipo_dato_campo_aux_c character varying(20),
    observacion character varying(200),
    estado integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
    fecha_reg timestamp(6) without time zone NOT NULL,
    user_mod_id integer,
    fecha_mod timestamp(6) without time zone,
    grupoarchivos_id integer,
    sexo character varying(1),
    tipo_proceso_contratacion integer
);


