CREATE FUNCTION f_relaborales_y_personas_nuevas_un_registro(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT solelabcontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT organigrama_regional character varying, OUT id_regional integer, OUT regional character varying, OUT regional_codigo integer, OUT id_departamento integer, OUT departamento character varying, OUT id_provincia integer, OUT provincia character varying, OUT id_localidad integer, OUT localidad character varying, OUT residencia character varying, OUT unidad_ejecutora character varying, OUT cod_ue integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT unidades_superiores character varying, OUT unidades_dependientes character varying, OUT partida integer, OUT fuente_codigo integer, OUT fuente character varying, OUT organismo_codigo integer, OUT organismo character varying, OUT observacion character varying, OUT estado integer, OUT estado_descripcion text, OUT estado_abreviacion text, OUT tiene_contrato_vigente integer, OUT id_eventual integer, OUT id_consultor integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT persona_user_reg_id integer, OUT persona_fecha_reg timestamp without time zone, OUT persona_user_mod_id integer, OUT persona_fecha_mod timestamp without time zone) RETURNS SETOF record
    LANGUAGE sql
    AS $$
/**********************************************************************************
 Nombre: f_relaborales_y_personas_nuevas_un_registro
 Descripción: Función para obtener los registros laborales en el sistema Oasis con la adición de personas que no tengan ni un sólo registro de relación laboral. 
	      La particularidad de esta función se da porque sólo muestra un registro por persona. siendo que si una persona tiene más de un registro sólo se muestra el último registro.	
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      08/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
select * from f_relaborales_y_personas_nuevas() t1
where fecha_ini = (select max(t2.fecha_ini) from relaborales t2 where t2.persona_id = t1.id_persona)
or fecha_ini is null;
$$;


ALTER FUNCTION public.f_relaborales_y_personas_nuevas_un_registro(OUT id_relaboral integer, OUT id_persona integer, OUT nombres character varying, OUT p_nombre character varying, OUT s_nombre character varying, OUT t_nombre character varying, OUT p_apellido character varying, OUT s_apellido character varying, OUT c_apellido character varying, OUT tipo_documento character varying, OUT ci character varying, OUT expd character, OUT num_complemento character, OUT fecha_nac date, OUT edad integer, OUT lugar_nac character varying, OUT genero character, OUT e_civil character varying, OUT item integer, OUT carrera_adm integer, OUT num_contrato character varying, OUT contrato_numerador_estado integer, OUT id_solelabcontrato integer, OUT solelabcontrato_regional_sigla character varying, OUT solelabcontrato_numero integer, OUT solelabcontrato_gestion integer, OUT solelabcontrato_codigo text, OUT solelabcontrato_user_reg_id integer, OUT solelabcontrato_fecha_sol timestamp without time zone, OUT fecha_ini date, OUT fecha_incor date, OUT fecha_fin date, OUT fecha_baja date, OUT fecha_ren date, OUT fecha_acepta_ren date, OUT fecha_agra_serv date, OUT motivo_baja character varying, OUT motivosbaja_abreviacion character varying, OUT descripcion_baja character varying, OUT descripcion_anu character varying, OUT id_cargo integer, OUT cargo_codigo character varying, OUT cargo character varying, OUT id_nivelsalarial integer, OUT nivelsalarial character varying, OUT nivelsalarial_resolucion_id integer, OUT numero_escala integer, OUT gestion_escala integer, OUT sueldo numeric, OUT id_procesocontratacion integer, OUT proceso_codigo character varying, OUT convocatoria_codigo character varying, OUT convocatoria_tipo character varying, OUT id_fin_partida integer, OUT fin_partida character varying, OUT id_condicion integer, OUT condicion character varying, OUT categoria_laboral character varying, OUT id_da integer, OUT direccion_administrativa character varying, OUT organigrama_regional_id integer, OUT organigrama_regional character varying, OUT id_regional integer, OUT regional character varying, OUT regional_codigo integer, OUT id_departamento integer, OUT departamento character varying, OUT id_provincia integer, OUT provincia character varying, OUT id_localidad integer, OUT localidad character varying, OUT residencia character varying, OUT unidad_ejecutora character varying, OUT cod_ue integer, OUT id_gerencia_administrativa integer, OUT gerencia_administrativa character varying, OUT id_departamento_administrativo integer, OUT departamento_administrativo character varying, OUT id_organigrama integer, OUT unidad_administrativa character varying, OUT organigrama_sigla character varying, OUT organigrama_orden integer, OUT id_area integer, OUT area character varying, OUT id_ubicacion integer, OUT ubicacion character varying, OUT unidades_superiores character varying, OUT unidades_dependientes character varying, OUT partida integer, OUT fuente_codigo integer, OUT fuente character varying, OUT organismo_codigo integer, OUT organismo character varying, OUT observacion character varying, OUT estado integer, OUT estado_descripcion text, OUT estado_abreviacion text, OUT tiene_contrato_vigente integer, OUT id_eventual integer, OUT id_consultor integer, OUT user_reg_id integer, OUT fecha_reg timestamp without time zone, OUT user_mod_id integer, OUT fecha_mod timestamp without time zone, OUT persona_user_reg_id integer, OUT persona_fecha_reg timestamp without time zone, OUT persona_user_mod_id integer, OUT persona_fecha_mod timestamp without time zone) OWNER TO postgres;

--
-- TOC entry 427 (class 1255 OID 22302)
-- Name: f_tiene_relaboral_vigente(integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION f_tiene_relaboral_vigente(i_id_persona integer, OUT o_resultado integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
BEGIN
/***************************************************************************************************************************************************
 Nombre: f_tiene_relaboral_vigente
 Descripción: Función para verificar si una persona tiene un registro de relación laboral en estado ACTIVO o EN PROCESO, si es así retorna 1, caso contrario 0.
 Historial de Creación y Modificaciones:
     Ver        Fecha         Autor                     Descripción
   ---------  ----------  ---------------  ------------------------------------
     1.0      17/10/2014  Javier Loza      Creación de la función.
****************************************************************************************************************************************************/
SELECT CASE WHEN COUNT(*)>0 THEN 1 ELSE 0 END INTO o_resultado FROM relaborales WHERE persona_id = i_id_persona and ESTADO>=1;
END;
$$;


ALTER FUNCTION public.f_tiene_relaboral_vigente(i_id_persona integer, OUT o_resultado integer) OWNER TO postgres;

--
-- TOC entry 414 (class 1255 OID 57805)
-- Name: process_relaborales_cargos(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION process_relaborales_cargos() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
    BEGIN
/**********************************************************************************
 Descripción: Función para actualizar la tabla de cargos en función de la adjudicación y baja de relaciones laborales
 Historial de Modificaciones:
   Ver        Fecha         Autor           Descripción
  ---------  ----------  ---------------  ---------------------------------------
     1.0      12/11/2014  Javier Loza     Creación de la función.
***********************************************************************************/
        IF (TG_OP = 'UPDATE' AND NEW.estado>=1) THEN
            UPDATE cargos set estado = 1,user_mod_id=NEW.user_mod_id,fecha_mod=current_timestamp where id = NEW.cargo_id;
            RETURN NEW;
        ELSIF (TG_OP = 'UPDATE' AND NEW.estado=0) THEN
            UPDATE cargos set estado = 0,user_mod_id=NEW.user_mod_id,fecha_mod=current_timestamp where id = NEW.cargo_id;
            RETURN NEW;
        ELSIF (TG_OP = 'INSERT' AND NEW.estado>=1) THEN
            UPDATE cargos set estado = 1,user_mod_id=NEW.user_reg_id,fecha_mod=current_timestamp  where id = NEW.cargo_id;
            RETURN NEW;
        END IF;
        RETURN NULL; 
    END;
$$;


ALTER FUNCTION public.process_relaborales_cargos() OWNER TO postgres;

SET default_tablespace = '';

SET default_with_oids = false;

