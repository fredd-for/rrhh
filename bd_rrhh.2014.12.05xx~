-- Name: relaborales; Type: TABLE; Schema: public; Owner: postgres; Tablespace: 
--

CREATE TABLE relaborales (
    id integer NOT NULL,
    persona_id integer NOT NULL,
    num_contrato character varying(30),
    da_id integer NOT NULL,
    regional_id integer NOT NULL,
    organigrama_id integer,
    ejecutora_id integer,
    procesocontratacion_id integer NOT NULL,
    cargo_id integer NOT NULL,
    certificacionitem_id integer,
    finpartida_id integer NOT NULL,
    carrera_adm integer,
    nivelsalarial_id integer NOT NULL,
    fecha_ini date NOT NULL,
    fecha_incor date,
    fecha_fin date,
    fecha_baja date,
    fecha_ren date,
    fecha_acepta_ren date,
    fecha_agra_serv date,
    motivobaja_id integer,
    descripcion_baja character varying(200),
    descripcion_anu character varying(200),
    solelabcontrato_id integer,
    solampliacioncontrato_id integer,
    observacion character varying(1000),
    pagado integer NOT NULL,
    estado integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
