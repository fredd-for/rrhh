CREATE TABLE relaboralesconsultores (
    id integer NOT NULL,
    relaboral_id integer NOT NULL,
    dir_ejecutivo_id integer NOT NULL,
    dir_adm_financiero_id integer,
    regional_id integer NOT NULL,
    fecha_emi timestamp(6) without time zone NOT NULL,
    cuce character varying(50),
    consultoria character varying(200) NOT NULL,
    cantidad integer NOT NULL,
    fecha_conv timestamp(6) without time zone NOT NULL,
    num_conf_sicoes integer,
    recomendacion character varying(20),
    fecha_rec timestamp(6) without time zone,
    objeto_causa character varying(500),
    nota_inicio_proceso character varying(200),
    informe_tecnico character varying(50),
    fecha_inf_tec timestamp(6) without time zone,
    informe_legal character varying(50),
    fecha_inf_leg timestamp(6) without time zone,
    certificacion_item_id integer,
    codigo_certificacion character varying(200),
    fecha_cert timestamp(6) without time zone,
    observacion character varying(200),
    estado integer NOT NULL,
    baja_logica integer NOT NULL,
    agrupador integer NOT NULL,
    user_reg_id integer NOT NULL,
    fecha_reg timestamp(6) without time zone NOT NULL,
    user_mod_id integer,
    fecha_mod timestamp(6) without time zone
);


